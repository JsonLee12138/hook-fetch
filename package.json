{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=hook-fetch && changeset publish",
    "typecheck": "turbo run typecheck",
    "ci:docs": "pnpm --filter=hook-fetch-docs run ci",
    "build:docs": "pnpm --filter=hook-fetch-docs run build",
    "dev:docs": "pnpm --filter=hook-fetch-docs run start",
    "build:core": "pnpm --filter=hook-fetch run build",
    "publish:beta": "pnpm --filter=hook-fetch run publish:beta",
    "publish:release": "pnpm --filter=hook-fetch run publish:release",
    "deploy:docs": "pnpm --filter=@jsonlee/docs run deploy",
    "lint-staged": "lint-staged",
    "cz": "cz",
    "cv:beta": "pnpm changeset pre enter beta --filter=hook-fetch",
    "tag": "changeset version && git add . && git commit -m 'chore(release): version packages' && deno run -A scripts/tag.ts && git push --follow-tags",
    "push:tag": "git push --follow-tags"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "cz-git": "^1.12.0",
    "deno": "^2.5.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "oxlint": "^1.16.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "name": "with-rolldown",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
